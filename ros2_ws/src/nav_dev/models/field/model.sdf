<?xml version="1.0" ?>
<sdf version="1.6">
  <model name="my_field" canonical_link='field'>
    <!--動かないエンティティであることを設定-->
    <static>true</static>
    <link name="field">
      <collision name="collision">
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://field/meshes/HaruRobo2024field.stl</uri>
          </mesh>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1.2</mu>
              <mu2>1.2</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <scale>1 1 1</scale>
            <uri>model://field/meshes/HaruRobo2024field.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
  </model>
</sdf>